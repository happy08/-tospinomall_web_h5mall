{
  "version": 1,
  "alias": "http://192.168.2.45:8000",
  "builds": [{
    "src": "nuxt.config.js",
    "use": "@nuxtjs/now-builder"
  }],
  "routes": [{
      "src": "/_nuxt/.+",
      "headers": {
        "Cache-Control": "max-age=31557600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}